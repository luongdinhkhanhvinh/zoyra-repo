{
	"name": "studio-ui",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build --base=/assets/studio/frontend/ && yarn copy-html-entry && yarn copy-studio-renderer",
		"build-studio-app": "node src/scripts/build.js",
		"copy-html-entry": "cp ../studio/public/frontend/index.html ../studio/www/studio.html",
		"copy-studio-renderer": "cp ../studio/public/frontend/renderer.html ../studio/templates/generators/studio_renderer.html",
		"serve": "vite preview",
		"extract-frappeui-types": "tsx src/scripts/tsToJSONGenerator.ts frappeui",
		"extract-studio-types": "tsx src/scripts/tsToJSONGenerator.ts studio"
	},
	"dependencies": {
		"@codemirror/lang-css": "^6.3.1",
		"@codemirror/lang-html": "^6.4.9",
		"@codemirror/lang-javascript": "^6.2.4",
		"@codemirror/lang-json": "^6.0.1",
		"@codemirror/lang-python": "^6.2.1",
		"@vitejs/plugin-vue": "^5.2.0",
		"@vueuse/components": "^10.11.1",
		"@vueuse/core": "^10.11.1",
		"ace-builds": "^1.36.2",
		"autoprefixer": "^10.4.2",
		"codemirror": "^6.0.1",
		"feather-icons": "^4.28.0",
		"frappe-ui": "0.1.149",
		"marked": "^15.0.6",
		"pinia": "^2.2.1",
		"thememirror": "^2.0.1",
		"typescript": "^5.8.3",
		"vite": "^5.4.11",
		"vue": "^3.5.14",
		"vue-codemirror": "^6.1.1",
		"vue-router": "^4.2.2",
		"vue-sonner": "^1.3.0",
		"vuedraggable": "^4.1.0"
	},
	"devDependencies": {
		"eslint": "^8.19.0",
		"eslint-plugin-vue": "^9.11.0",
		"postcss": "^8.4.5",
		"prettier": "^2.8.8",
		"prettier-plugin-tailwindcss": "^0.2.8",
		"tailwindcss": "^3.4.3",
		"ts-json-schema-generator": "2.3.0",
		"tsx": "^4.19.2"
	}
}
